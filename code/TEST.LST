         ;***** Constants*************************************
          .equ	preset=206			;T/C0 Preset constant (256-50)R=20k,c=0.01uf	
         ;***** A/D converter Global Registers****************
          .def	result=r16			;轉換結果
          .def	temp1=r17			;變數 register
          .def	temp2=r18
          .def	avgVoltage=R19
          .def	Accumulator=r20
          .def	loopReg=r21
          .cseg
          	.org $0000
000000 c02e      	rjmp RESET      	;Reset 中斷向量
          	.org OVF0addr
000006 c004      	rjmp ANA_COMP   	;Timer0 overflow 中斷向量
          	.org ACIaddr
00000a c000      	rjmp ANA_COMP   	;Analog comparator 中斷向量
         ;**********************************************************************
          ANA_COMP:       
00000b b702      		in      result,TCNT0    ;Load timer value
00000c 98c2      		cbi     PORTB,PB2       ;電容開始放電
00000d 9ac3      		sbi     PORTB,PB3       
00000e 2711      		clr     temp1    		;Stop timer0
00000f bf13      		out     TCCR0,temp1
000010 5c0f      		subi    result,preset+1 ;減去預設常數
000011 9468      		set						;Set T flag	(=1,轉換完成)
         								;			(=0,正在轉換)
000012 98c3      		cbi     PORTB,PB3		;fast discharge 禁能(大約1us時間)
000013 9518      		reti                    ;Return
         ;***********************************************************************
          convert_init:
000014 2755      		clr		loopReg
000015 2744      		clr		Accumulator
000016 2733      		clr		avgVoltage
000017 ef1f      		ldi		temp1,$ff		;set port D as output
000018 bb11      		out		DDRD,temp1		
000019 9aba      		sbi		ddrb,2			;充放電設定位元設為輸出
00001a 9abb      		sbi		ddrb,3			;快速放電控制位元設為輸出
00001b 98c2      		cbi     PORTB,PB2       ;電容開始放電
00001c 9ac3      		sbi		PORTB,PB3
00001d e012      		ldi     temp1,$02      	;Enable Timer0 interrupt
00001e bf19      		out     TIMSK,temp1		
00001f 98c3      		cbi		PORTB,PB3
000020 9508      		ret						;Return
         ;************************************************************************
          AD_convert:
000021 ec2e      		ldi     temp2,preset   ;Clear counter
000022 bf22      		out     TCNT0,temp2    ;and load offset value
000023 94e8      		clt						;Clear T flag
000024 e022      		ldi		temp2,$02		;預除 f/8 and 開始計時
000025 bf23      		out     TCCR0,temp2    
000026 9ac2      		sbi     PORTB,PB2       ;Start charging
000027 9508      		ret						;Return
         ;******************RC discharge time*********************
          Delay:	
000028 2722      		clr		temp2		
000029 ef1d      		ldi		temp1,$fd	
          loop1:	
00002a 9523      		inc		temp2		
00002b f7f1      		brne	loop1		
00002c 9513      		inc 	temp1		
00002d f7e1      		brne 	loop1		
00002e 9508      		ret
         
         ;**************main program*******************************************
          RESET:	
00002f ed1f      		ldi		temp1,low(RAMEND)		;堆疊設定
000030 bf1d      		out		SPL,temp1
000031 dfe2      		rcall	convert_init	;A/D 轉換設定
000032 9478      		sei						;Enable interrupt
000033 dff4      again:	rcall	Delay			;放電延遲
000034 dfec      		rcall	AD_convert		;開始轉換
000035 f7fe      Wait:	brtc	Wait			;等待
000036 cffc      		rjmp	again			;again
